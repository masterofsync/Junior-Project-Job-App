@model WcfAESJobs.Client.WebService.JobTemplates

@{
    ViewBag.Title = "Delete";
}

<div class="curveunderheader"><span class="glyphicon glyphicon-folder-open"></span>&nbsp;&nbsp;Delete Job Template</div>
<br />


<h3>Are you sure you want to delete this?</h3>
<div>
    <hr />
    <h5>@ViewBag.StatusMessage</h5>
    <dl class="dl-horizontal">

        <dt>
            Title
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Job_Title)
        </dd>

        <dt>
            Description
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Job_Description)
        </dd>

        @*<dt>
            @Html.DisplayNameFor(model => model.Job_ID)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Job_ID)
        </dd>*@

        <dt>
            Qualification
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Job_Qualifications)
        </dd>



    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Delete" class="btn btn-default" /> &nbsp;
            @Html.ActionLink("Back to List", "Index",null,new{@class="btn btn-default"})
        </div>
    }
</div>
