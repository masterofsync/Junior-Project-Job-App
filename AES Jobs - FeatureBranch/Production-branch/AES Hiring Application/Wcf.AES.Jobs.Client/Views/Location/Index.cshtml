@model IEnumerable<WcfAESJobs.Client.WebService.StoreLocations>

@{
    ViewBag.Title = "AES Locations";
}
<div class="curveunderheader"><span class="glyphicon glyphicon-folder-open"></span>&nbsp;&nbsp;Locations</div>
<br />

<p>
    @Html.ActionLink("Create New", "Create", null,new { @class = "btn btn-default" })
</p>
<table class="table table-striped">
    <tr>
        <th>
            Location City
        </th>
        <th>
            Location Name
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Location_City)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Location_Name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit",new { Location_ID = item.Location_ID }, new { @class = "btn btn-default btn-sm" })
        </td>
    </tr>
}

</table>
